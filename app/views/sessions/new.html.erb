<h1>Login</h1>
<%= "<span>#{@msg}</span>".html_safe if @msg %>
<%= form_for(:sessions, url: login_path) do |f| %>
	<%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Log in"%>
<% end %>
<span>No account? <a href="/sign_up">Sign Up</a></span>
